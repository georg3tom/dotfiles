{
    "diagnostic.errorSign": "✖",
    "diagnostic.hintSign": "·",
    "diagnostic.infoSign": "·",
    "diagnostic.warningSign": "·",
    "suggest.snippetIndicator": "",
    "coc.source.ultisnips.priority": 5,
    "languageserver": {
        "dockerfile": {
            "command": "docker-langserver",
            "filetypes": ["Dockerfile"],
            "args": ["--stdio"]
        },
        "lua": {
            "command": "lua-lsp",
            "filetypes": ["lua"]
        }
    },
    "python.pythonPath": "python3",
    "python.formatting.provider": "black"
}
